#ifndef PWM_H_
#define PWM_H_

#define LCD_BACKLITE_BRIGHTNESS (TB3CCR1)

#define RIGHT_FORWARD_SPEED     (TB3CCR2)

#define LEFT_FORWARD_SPEED      (TB3CCR3)

#define RIGHT_REVERSE_SPEED     (TB3CCR4)

#define LEFT_REVERSE_SPEED      (TB3CCR5)




#endif /* PWM_H_ */
